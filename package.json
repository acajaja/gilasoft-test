{
  "name": "gilasoft-test",
  "version": "1.0.0",
  "description": "Demo application for job interview.",
  "private": true,
  "scripts": {
    "build:dev": "NODE_ENV=development webpack",
    "build:watch": "NODE_ENV=development webpack -w",
    "prestart": "npm run build",
    "dev:run": "NODE_ENV=development concurrently \"nodemon -V ./dist/js/frontend.bundle.js\" \"nodemon -V ./dist/js/api.bundle.js\"",
    "dev:watch": "NODE_ENV=development concurrently \"nodemon -w src -w public -V -e js ./dist/js/frontend.bundle.js\" \"nodemon -e js -w src -w dist -V ./dist/js/api.bundle.js\"",
    "start": "concurrently \"node ./dist/js/frontend.bundle.js\" \"node ./dist/js/api.bundle.js\"",
    "test": "jest"
  },
  "engine": {
    "node": "18.x",
    "npm": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/acajaja/gilasoft-test.git"
  },
  "author": "Clif Jackson",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/acajaja/gilasoft-test/issues"
  },
  "homepage": "https://github.com/acajaja/gilasoft-test#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^7.4.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "lowdb": "^4.0.0",
    "moment": "^2.29.4",
    "mongodb": "^4.10.0",
    "pino": "^8.6.1",
    "pino-http": "^8.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "babel-jest": "^29.2.0",
    "babel-loader": "^8.2.5",
    "jest": "^29.2.0",
    "nodemon": "^2.0.20",
    "path": "^0.12.7",
    "superagent": "^8.0.2",
    "supertest": "^6.3.0",
    "url": "^0.11.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
