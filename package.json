{
  "name": "gilasoft-test",
  "version": "1.0.0",
  "description": "Demo application for job interview.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "prestart": "npm run build",
    "dev:run": "NODE_ENV=development nodemon ./server.js",
    "dev:watch": "NODE_ENV=development concurrently \"nodemon -w src -w public -V ./server.js\" \"nodemon -w src -w public -V ./api.js\"",
    "start": "node ./server.js",
    "test": "test"
  },
  "engine": {
    "node": "18.x",
    "npm": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/acajaja/gilasoft-test.git"
  },
  "author": "Clif Jackson",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/acajaja/gilasoft-test/issues"
  },
  "homepage": "https://github.com/acajaja/gilasoft-test#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^7.4.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongodb": "^4.10.0",
    "pino": "^8.6.1",
    "pino-http": "^8.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4"
  }
}
